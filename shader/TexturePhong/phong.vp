varying vec3 N;
varying vec3 v;
varying vec2 tc;
varying vec2 tc2;

void main(void) {
   v = vec3(gl_ModelViewMatrix * gl_Vertex);
   N = normalize(gl_NormalMatrix * gl_Normal);
   tc = vec2(gl_MultiTexCoord0);
   tc2 = vec2(gl_MultiTexCoord1);
   gl_Position = gl_ModelViewProjectionMatrix * gl_Vertex;
}
