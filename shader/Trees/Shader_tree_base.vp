// vertex shader program
varying mat4 model;
varying mat3 n_model;
varying vec2 tc;
varying vec3 vert_normal;

//----------------------------------------------------------------------------------------------MAIN--VP
void main( void ) {
    tc = gl_MultiTexCoord0.xy;
    model = gl_ModelViewProjectionMatrix;
    n_model = gl_NormalMatrix;
    gl_Position = gl_Vertex;
    vert_normal = gl_Normal;
}
